name_F = (input())
price_kg_F = int(input())
weight2_F = int(input())
money_F = int(input())
check_F = ("Чек")
cost_F = price_kg_F * weight2_F
change_F = money_F - cost_F
nul = 0
print(check_F)
print(f"{name_F} - {weight2_F}кг - {price_kg_F}руб/кг")
print(f"Итого: {cost_F}руб")
print(f"Внесено: {money_F}руб")
if change_F < 0: 
    print(f"Сдача: {nul}руб") 
else: 
    print(f"Сдача: {change_F}руб")
